{
    "compilerOptions": {
      "experimentalDecorators": true,
      "target": "es2016",
      "module": "commonjs",
      "baseUrl": "src",
      "resolveJsonModule": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "strictPropertyInitialization": false,
    }
}